on init
    # Disable framebuffer alignment
    write /sys/module/msm_fb/parameters/align_buffer 0

on boot

service rmt_storage /sbin/rmt_storage

service charger /sbin/healthd -c
    class charger
    critical
    seclabel u:r:healthd:s0

on charger
    start rmt_storage
    class_start charger
