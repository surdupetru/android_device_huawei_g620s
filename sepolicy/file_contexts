# Camera
/data/cam_socket([0-9])+                                    u:object_r:camera_socket:s0

# CMHW
/sys/devices/virtual/graphics/fb0/hbm                       u:object_r:display_sysfs:s0
/sys/devices/virtual/graphics/fb0/rgb                       u:object_r:display_sysfs:s0
/sys/devices/virtual/timed_output/vibrator/vtg_level        u:object_r:vibeamp_sysfs:s0

/persist/.genmac                                u:object_r:wifi_data_file:s0
/persist/.bt_nv.bin                             u:object_r:bluetooth_data_file:s0

/dev/smd3                                       u:object_r:hci_attach_dev:s0

# NFC
/dev/nfc-nci    	 			u:object_r:nfc_device:s0
